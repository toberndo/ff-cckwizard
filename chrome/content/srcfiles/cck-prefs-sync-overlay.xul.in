<?xml version="1.0"?>
<overlay id="cckPrefsSyncOverlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
<prefpane id="paneSync" onpaneload="gSyncPane.init();
(function() {
    try {
      if (!Services.prefs.getBoolPref('services.sync.enabled'))
        document.getElementById('weavePrefsDeck').selectedPanel = document.getElementById('cckSyncDisabled');
    } catch (ex) {}
})();
">
</prefpane>
  <deck id="weavePrefsDeck">
    <vbox id="cckSyncDisabled" insertafter="needsUpdate" align="center">
      <spacer flex="1"/>
      <description>
        Sync has been disabled by your administrator.
      </description>
      <spacer flex="3"/>
    </vbox>
  </deck>
</overlay>
